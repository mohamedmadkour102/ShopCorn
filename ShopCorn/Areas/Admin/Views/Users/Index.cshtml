@model IEnumerable<ApplicationUser>
@{
    ViewBag.PageTitle = "Users Managment";
    ViewBag.CardTitle = "View Users";
    // Layout = "~/Views/Shared/_Dashboard.cshtml";
    // ViewData["Title"] = "Index";
}


<div>
    <a class="btn btn-primary" href="/Identity/Account/Register">Create new User</a>
</div>

<br />
<table class="table table-hover" width="100">
    <tr>
        <td>Name</td>
        <td>Email</td>
        <td>PhoneNumber</td>
        <td>Actions</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>
             <a class="btn btn-success" asp-action="LockUnlock"> <i class="fas fa-lock-open"></i> </a>  
             <a class="btn btn-danger" href="/Category/Delete/@item.Id">Delete</a>
            </td>
        </tr>
    }
</table>